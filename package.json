{
	"name": "guanatos-app",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite && pnpm run css:watch",
		"build": "pnpm run build:css && vite build",
		"preview": "vite preview",
		"format": "prettier --write .",
		"lint": "eslint . --ext .js .jsx --fix",
		"build:css": "postcss src/assets/css/index.css -o src/assets/css/styles.css",
		"css:watch": "postcss src/assets/css/index.css -o src/assets/css/styles.css --watch"
	},
	"dependencies": {
		"@react-google-maps/api": "2.19.2",
		"axios": "1.4.0",
		"formik": "2.4.3",
		"framer-motion": "10.15.1",
		"i18next": "23.4.4",
		"prop-types": "15.8.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-i18next": "13.1.2",
		"react-router-dom": "6.15.0",
		"react-toastify": "9.1.3",
		"yup": "1.2.0"
	},
	"devDependencies": {
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"@vitejs/plugin-react": "4.0.4",
		"autoprefixer": "10.4.14",
		"eslint": "8.47.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-config-standard": "17.1.0",
		"eslint-plugin-import": "2.28.0",
		"eslint-plugin-n": "16.0.1",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-react": "7.33.1",
		"postcss": "8.4.27",
		"postcss-cli": "10.1.0",
		"prettier": "3.0.1",
		"tailwindcss": "3.3.3",
		"vite": "4.4.9"
	}
}
