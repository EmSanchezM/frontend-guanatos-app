{
	"name": "guanatos-app",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "npm run build:css && vite build",
		"preview": "vite preview",
		"format": "prettier --write .",
		"lint": "eslint . --ext .js .jsx --fix",
		"build:css": "postcss src/assets/css/index.css -o src/assets/css/styles.css",
		"css:watch": "postcss src/assets/css/index.css -o src/assets/css/styles.css --watch"
	},
	"dependencies": {
		"axios": "0.27.2",
		"formik": "2.2.9",
		"framer-motion": "6.3.3",
		"i18next": "21.8.2",
		"prop-types": "15.8.1",
		"react": "18.0.0",
		"react-dom": "18.0.0",
		"react-i18next": "11.16.9",
		"react-router-dom": "6.3.0",
		"yup": "0.32.11"
	},
	"devDependencies": {
		"@types/react": "18.0.0",
		"@types/react-dom": "18.0.0",
		"@vitejs/plugin-react": "1.3.0",
		"autoprefixer": "10.4.7",
		"eslint": "8.15.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-standard": "17.0.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-n": "15.2.0",
		"eslint-plugin-promise": "6.0.0",
		"eslint-plugin-react": "7.29.4",
		"postcss": "8.4.13",
		"postcss-cli": "9.1.0",
		"prettier": "2.6.2",
		"tailwindcss": "3.0.24",
		"vite": "2.9.9"
	}
}
